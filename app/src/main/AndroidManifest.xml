<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyLibrary">
        <activity
            android:name=".WebviewActivity"
            android:exported="false" />
        <activity
            android:name=".FavouriteBookActivity"
            android:label="@string/activity_favourite_book"
            android:exported="false" />
        <activity
            android:name=".CurrentlyReadbookActivity"
            android:label="@string/activity_currently_readbook"
            android:exported="false" />
        <activity
            android:name=".WanttoreadActivity"
            android:label="@string/activity_wanttoread"
            android:exported="false" />
        <activity
            android:name=".AlreadyReadbookActivity"
            android:label="@string/activity_already_read"
            android:exported="false" />
        <activity
            android:name=".BookActivity"
            android:label="@string/activity_book"
            android:exported="false" />
        <activity
            android:name=".AllBooksActivity"
            android:label="@string/activity_all_books"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>